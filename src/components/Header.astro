---
 
import HerramientasIa from "./HerramientasIa.astro";
 
import {NavMobil} from "./react/NavMobil";
import NavLinks from "./NavLinks.astro";

const navData = [
 
  { id: 2, nombre: "Envia tu proyecto", ruta: "/envia-tu-proyecto/" },
  { id: 3, nombre: "Blog de IA", ruta: "/blog/" },
  { id: 4, nombre: "Contacto", ruta: "/contacto/" },
];
---

 
  <nav
    class="w-full md:max-w-screen-xl flex flex-row items-center justify-between mx-auto p-4"
  >
  <div>
    <a href="/" class="flex items-center space-x-3 rtl:space-x-reverse">
      <img
          src="/assets/logo.png"
          class="mr-3 w-128 md:w-64 rounded-md"
          alt="Directorio Ia Logo"
        />
       
    </a>
  </div>
    <div class="hidden w-full md:block md:w-auto menudrop">
      <ul
        class="flex flex-col font-medium p-4 md:p-2 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:space-x-8 rtl:space-x-reverse md:flex-row md:mt-0 md:border-0 md:bg-white dark:bg-gray-800 md:dark:bg-gray-900 dark:border-gray-700"
      >
        
        <li>
          <HerramientasIa />
        </li>
        
        {navData.map((item) => <NavLinks href={item.ruta} text={item.nombre} />)}
         
      </ul>
    </div>
    <NavMobil client:visible />
  </nav>
 
<script>
  import './js/navlink.js'
</script>

